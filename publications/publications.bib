	journal = {Protein Engineering Design and Selection},
	author = {Stadler, Lukas Kurt Josef and Hoffmann, Toni and Tomlinson, Darren Charles and Song, Qifeng and Lee, Tracy and Busby, Michael and Nyathi, Yvonne and Gendra, Elisenda and Tiede, Christian and Flanagan, Keith and Cockell, Simon J. and Wipat, Anil and Harwood, Colin and Wagner, Simon D. and Knowles, Margaret A. and Davis, Jason J. and Keegan, Neil and Ferrigno, Paul Ko},
	month = sep,
	year = {2011},
	pmid = {21616931},
	keywords = {circular dichroism, epitope recognition, microarray, peptide aptamer, scaffold protein},
	pages = {751--763},
	file = {Full Text PDF:/Users/sjcockell/Library/Application Support/Zotero/Profiles/b4j54irn.default/zotero/storage/3XIJABTK/Stadler et al. - 2011 - Structure−function studies of an engineered scaffo.pdf:application/pdf;Snapshot:/Users/sjcockell/Library/Application Support/Zotero/Profiles/b4j54irn.default/zotero/storage/F5ADRJCU/751.html:text/html}
